<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.5" createdBy="Projeto HASDLP - https://github.com/jesobreira/hasdlp" modifiedIn="OpenLP 2.0.5" modifiedDate="2015-04-24T02:42:01">
  <properties>
    <titles>
      <title>167 - Vem a Cristo</title>
    </titles>
    <authors>
      <author>HASD</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines><![CDATA[
		Vem a Cristo
	  ]]></lines>
    </verse>
<verse name="v1b">
      <lines><![CDATA[
		1. Vem a Cristo, vem agora,
	  ]]></lines>
    </verse>
<verse name="v1c">
      <lines><![CDATA[
		CORO: Vem, ó, vem, pecador! 
      Vem, vem, confiantemente, 
      A Jesus, o Senhor!
	  ]]></lines>
    </verse>
<verse name="v1d">
      <lines><![CDATA[
		2. Cristo pode, sim, salvar-te,
	  ]]></lines>
    </verse>
<verse name="v1e">
      <lines><![CDATA[
		3. Ele almeja perdoar-te,
	  ]]></lines>
    </verse>
<verse name="v1f">
      <lines><![CDATA[
		4. Tua entrega não rejeita,
	  ]]></lines>
    </verse>

  </lyrics>
</song>
